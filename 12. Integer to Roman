class Solution:
    def intToRoman(self, num: int) -> str:
        
        digit_1 = {'1': 'I', '2': 'II', '3': 'III', '4': 'IV', '5': 'V', '6': 'VI', '7': 'VII', '8': 'VIII', '9': 'IX'}
        digit_2 = {'1': 'X', '2': 'XX', '3': 'XXX', '4': 'XL', '5': 'L', '6': 'LX', '7': 'LXX', '8': 'LXXX', '9': 'XC'}
        digit_3 = {'1': 'C', '2': 'CC', '3': 'CCC', '4': 'CD', '5': 'D', '6': 'DC', '7': 'DCC', '8': 'DCCC', '9': 'CM'}
        digit_4 = {'1': 'M', '2': 'MM', '3': 'MMM'}
        string = str(num)
        l = [digit_1, digit_2, digit_3, digit_4]
        L = []
        for i in range(len(string)):
            L.append(l[len(string) - 1 - i][string[i]])

        return ''.join(L)
