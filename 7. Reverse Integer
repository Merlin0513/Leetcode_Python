class Solution:
    def reverse(self, x: int) -> int:
        
        s = list(str(x))

        l1, l2 = [], []
        for i in range(10):
            l1.append(str(i))

        if s[0] in l1:
            s.reverse()

        else:
            l2.append(s[0])
            s1 = s[1:]
            s1.reverse()
            s = l2 + s1

        if s[0] == '0':
            s[0] = ''

        return int(''.join(s))
