class Solution:
    def longestCommonPrefix(self, strs: List[str]) -> str:
        
        l, L = [], []
        
        for i in range(len(strs[0])):
            try:
                for s in strs:
                    l.append(s[i] == (strs[0])[i])
                if False in l:
                    break
                else:
                    L.append((strs[0])[i])
            except IndexError:
                break
                
        return ''.join(L)
